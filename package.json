{
  "name": "py-inline-sql",
  "displayName": "py-inline-sql",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "scripts": {
    "convert": "node ./convert.js"
  },
  "contributes": {
    "languages": [
      {
        "id": "inline-sql",
        "aliases": [
          "inline-sql",
          "inline-sql"
        ],
        "extensions": [
          ".psql"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "path": "./syntaxes/inline-sql.tmLanguage.json",
        "scopeName": "highlight-string-code",
        "injectTo": [
          "source.python"
        ],
        "embeddedLanguages": {
          "meta.embedded.block.sql": "sql"
        },
        "tokenTypes": {
          "meta.*": "other"
        }
      },
      {
        "path": "./syntaxes/highlight-string-code.json",
        "scopeName": "sql-string-highlight",
        "injectTo": [
          "source.python"
        ],
        "embeddedLanguages": {
          "meta.embedded.block.sql": "sql"
        },
        "tokenTypes": {
          "meta.*": "other"
        }
      }
    ]
  },
  "devDependencies": {
    "js-yaml": "^4.1.0"
  }
}