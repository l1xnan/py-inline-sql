{
  "name": "py-inline-sql",
  "displayName": "Inline SQL for Python",
  "description": "Inline SQL for Python",
  "version": "0.0.1",
  "publisher": "l1xnan",
  "private": true,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/l1xnan/py-inline-sql"
  },
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "activationEvents": [
    "onLanguage:python"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "py-inline-sql.sqlInlineFormat",
        "title": "Inline SQL Format"
      }
    ],
    "grammars": [
      {
        "path": "./syntaxes/highlight-sql-string.tmLanguage.json",
        "scopeName": "python-sql.injection",
        "injectTo": [
          "source.python"
        ],
        "embeddedLanguages": {
          "meta.embedded.block.sql": "sql"
        }
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "py-inline-sql.sqlInlineFormat",
          "when": "editorHasSelection"
        }
      ],
      "editor/context": [
        {
          "command": "py-inline-sql.sqlInlineFormat",
          "when": "editorHasSelection",
          "group": "1_modification"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint \"src/**/*.ts\"",
    "watch": "tsc -watch -p ./",
    "convert": "node ./convert.js"
  },
  "devDependencies": {
    "@types/node": "^16.18.38",
    "@types/vscode": "^1.73.0",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "eslint": "^8.26.0",
    "js-yaml": "^4.1.0",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "@sqltools/formatter": "^1.2.5",
    "prettier": "^3.0.0",
    "prettier-plugin-sql-cst": "^0.8.2",
    "sql-formatter": "^12.2.3"
  }
}
